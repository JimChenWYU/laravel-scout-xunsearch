{
    "name": "jimchen/laravel-scout-xunsearch",
    "description": "Xunsearch Driver for Laravel Scout.",
    "license": "MIT",
    "authors": [
        {
            "name": "JimChen",
            "email": "imjimchen@163.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/scout": "^8.0",
        "hightman/xunsearch": "^1.4",
        "donatj/php-ini-builder": "^0.1.4",
        "symfony/polyfill-php80": "^1.22"
    },
    "autoload": {
        "psr-4": {
            "JimChen\\LaravelScout\\XunSearch\\": "src"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
